<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="Controller.RankingsPageFXController"
            prefHeight="700.0" prefWidth="1200.0">
    <children>
        <SplitPane dividerPositions="0.27" prefHeight="700.0" prefWidth="1200.0">
            <items>
                <AnchorPane fx:id="leftPane" minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0" style="-fx-background-color: #BBA5E1;">
                    <children>
                        <Button fx:id="goBackButton" mnemonicParsing="false" onAction="#handleGoBackButton" prefHeight="40.0" prefWidth="70.0" />
                        <Label layoutX="35.0" layoutY="250.0" text="Rankings by genre:" textFill="WHITE">
                            <font>
                                <Font name="Avenir Next Bold" size="20.0" />
                            </font>
                        </Label>
                    </children>
                </AnchorPane>
                <AnchorPane fx:id="rightPane" minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0" style="-fx-background-color: #FFFFFF;" />
            </items>
        </SplitPane>
    </children>

</AnchorPane>
